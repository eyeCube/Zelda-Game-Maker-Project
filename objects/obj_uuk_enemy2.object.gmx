            <string>///attacking

if dead=1 exit
if stunned&gt;0 exit
if global.show_inventory=1 exit
if anim&lt;2 exit



var linkdist;
linkdist=distance_to_object(obj_link)





if can_jump&lt;=0
{
    if linkdist&lt;jump_dist
    or hit&gt;0
    {
        if image_index=0
        {
            image_index=1
            alarm[0]=15
            jump=1
            z_change=jump_height
            jump_dir=point_direction(x,y,center_x,center_y)+random(jump_deviation*2)-jump_deviation
            can_jump=jump_cooldown
            if life&lt;life_max/2
            {
                can_jump=jump_cooldown_hurt
            }
            
            can_fire=max(can_fire,25)
            
            if point_direction(x,y,obj_link.x,obj_link.y)&gt;=90
            and point_direction(x,y,obj_link.x,obj_link.y)&lt;=270
            {
                image_index=2
            }
            
            exit
        }
        else
        can_jump+=10
    }
}


if jump=0
if can_fire&lt;=0
{
    can_fire=fire_cooldown
    
    
    
    {
    
    if choose(0,1)=1
    {
        obj=instance_create(x,y,obj_enemy_bolt)
        obj.lobbed=1
        obj.direction=point_direction(x,y,obj_link.x,obj_link.y)+accuracy_deviation-random(accuracy_deviation*2)
        obj.move_speed=lob_speed_min+((linkdist&gt;lob_speed_change_dist)*(lob_speed_med-lob_speed_min))+((linkdist&gt;lob_speed_change_dist2)*(lob_speed_max-lob_speed_med))
        obj.sprite_index=lob_sprite
        obj.spell=lob_type
        obj.damage=lob_damage
        obj.deflectable=deflectable
        obj.create_particles=create_particles
        obj.gravity_intensity=gravity_intensity
        obj.z_change=lob_jump
        obj.jump=1
        obj.z=16
        obj.fragility=lob_fragile
        obj.bounciness=lob_bounce
        obj.irregular=irregular
        obj.image_xscale=-1
    }
    else
    {
        obj=instance_create(x,y,obj_enemy_bolt)
        obj.lobbed=1
        obj.direction=point_direction(x,y,obj_link.x,obj_link.y)+accuracy_deviation-random(accuracy_deviation*2)
        obj.move_speed=lob_speed_min2+((linkdist&gt;lob_speed_change_dist2)*(lob_speed_med2-lob_speed_min2))+((linkdist&gt;lob_speed_change_dist2_2)*(lob_speed_max2-lob_speed_med2))
        obj.sprite_index=lob_sprite2
        obj.spell=lob_type2
        obj.damage=lob_damage2
        obj.deflectable=deflectable
        obj.create_particles=0
        obj.gravity_intensity=gravity_intensity
        obj.z_change=lob_jump
        obj.jump=1
        obj.z=16
        obj.fragility=0
        obj.bounciness=lob_bounce
        obj.irregular=irregular
        obj.image_xscale=-1
    }
    
    
    
    
    if point_direction(x,y,obj_link.x,obj_link.y)&gt;=90
    and point_direction(x,y,obj_link.x,obj_link.y)&lt;=270
    {
        image_index=2
        obj.image_xscale=1
    }
    
    image_index=1
    alarm[0]=alarm0
    if life&lt;life_max/2
    {
        alarm[0]=alarm0hurt
        can_jump=max(alarm0hurt+5,can_jump)
        can_fire=fire_cooldown_hurt
    }
    else
    can_jump=max(alarm0+5,can_jump)
    
    }
    
    if linkdist&gt;lob_speed_change_dist
    {
        alarm[0]+=alarm0_1
        can_fire+=alarm0_1
    }
    if linkdist&gt;lob_speed_change_dist2
    {
        alarm[0]+=alarm0_2
        can_fire+=alarm0_2
    }
    
}
